<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.q.reminder.reminder.mapper.UserGroupMapping">
    <insert id="saveBatchAll">
        replace into fs_user_group(chat_id, member_id) values
        <foreach collection="list" item="item" separator=",">
            (#{item.chatId}, #{item.memberId})
        </foreach>
    </insert>
</mapper>
